# Example environment variables for the backend
PORT=3001
DATABASE_URL=postgres://username:password@hostname:5432/selfupgrade
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_AUDIENCE=your-api-audience
AUTH0_CLIENT_ID=your-auth0-client-id
FRONTEND_ORIGIN=http://localhost:8080
DEV_BYPASS_AUTH=false
AUTH0_TEST_USER=auth0|123456789

# Security: Encryption key for sensitive data (32 bytes = 64 hex characters)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_KEY=your-64-character-hex-encryption-key-here

# Production settings
NODE_ENV=production
